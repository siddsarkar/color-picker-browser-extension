{
  "name": "Pick Color",
  "version": "3.0.0",
  "description": "Pick any color from screen",
  "author": "Siddhartha Sarkar",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.ts"
  },
  "key": "",
  "permissions": ["contextMenus", "sidePanel", "clipboardWrite", "storage"],
  "icons": {
    "48": "assets/icons/icon48.png",
    "96": "assets/icons/icon96.png",
    "144": "assets/icons/icon128.png",
    "240": "assets/icons/icon256.png"
  },
  "action": {
    "default_icon": "assets/icons/icon48.png",
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "features/color-picker/presentation/views/sidepanel/index.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+6"
      }
    }
  }
}
